   "source": [
    "import matplotlib.pyplot as plt\n",
    "%pylab inline\n",
    "x = [0,1,2,3]\n",
    "y = [0,1,2,3]\n",
    "A = np.vstack([x, np.ones(len(x))]).T\n",
    "m, c = np.linalg.lstsq(A, y)[0]\n",
    "print (m,c)\n",
    "plt.plot(x, y, 'o', label='Original data', markersize=10)\n",
    "plt.plot(x, m*x + c, 'r', label='Fitted line')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
